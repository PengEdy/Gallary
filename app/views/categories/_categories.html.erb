<div class="large-2 pull-10 columns sidebar">
  <%= link_to image_tag("logo3.png", :class => "logo ie"), root_path, :class => "logo" %>
  <li><br></li>
  <ul class="custom-side-nav">
    <% Category.all.each do |category| %>
    <li><%= link_to category.name.upcase, category_path(category), :class => "right" %></li>
    <% end %>
  </ul>
  <ul class="custom-side-nav info">
    <li>&nbsp;</li>
    <li><%= link_to "CONTACT US", contact_path, :class => "right" %></li>
    <li><%= link_to "ABOUT", about_path, :class => "right" %></li>
  </ul>
</div>
