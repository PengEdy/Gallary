<% @videos.each do |video| %>
<h3><%= video.title %></h3>
<p>
  <iframe width="800" height="450" src="//www.youtube.com/embed/<%= video.source %>" frameborder="0" allowfullscreen></iframe>
</p>
<br>
<% end %>
</div>
